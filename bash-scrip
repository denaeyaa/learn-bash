read -p "apa yang mau diinstall bruh?" well

if command -v "$well" &> /dev/null; then
        echo "udah ke install loh"
                else

read -p "belom ada. apa mau lanjut install? [yes/no]" permision

        case $permision in (y|Y|yes)
        apt-get install $well -y
;;
        *)
        echo "dibatalin"
;; esac
fi


